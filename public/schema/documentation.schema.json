{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Documentation",
  "definitions": {
    "Documentation": {
      "type": "object",
      "properties": {
        "entries": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Entry"
          }
        },
        "types": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Type"
          }
        }
      },
      "required": ["entries", "types"]
    },
    "Entry": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      },
      "required": ["name", "path", "type"]
    },
    "Type": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "count": {
          "type": "integer"
        },
        "slug": {
          "type": "string"
        }
      },
      "required": ["count", "name", "slug"]
    }
  }
}
